@model Carousel

@if (Model != null)
{
    <div class="banner" style="background-image:url('@Model.ImageUrl')">
        <div class="container">
            <div class="title-page">
                <h1>@Model.Title</h1>
                <p>
                    @Model.Description
                </p>
            </div>
        </div>
    </div>
}
