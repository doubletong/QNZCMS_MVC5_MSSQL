@model CaseSetIM

@using (Ajax.BeginForm("CaseSet", null, new AjaxOptions()
{
    OnBegin = "onBegin",
    OnComplete = "onComplete",
    //   Confirm = "Are you sure you want to save this User?"
    OnSuccess = "onSuccess"

}, new { @class = "form-horizontal", role = "form" }))
{

    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <div class="form-group">
        @Html.LabelFor(model => model.EnableCaching, new { @class = "col-md-2 control-label" })
        <div class="col-md-5">
            <div class="checkbox">
                <label>
                    @Html.CheckBoxFor(model => model.EnableCaching)  @Html.DisplayNameFor(model => model.EnableCaching)
                </label>
            </div>
        </div>
        <div class="col-md-5">
            @Html.ValidationMessageFor(model => model.EnableCaching)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.CacheDuration, new { @class = "col-md-2 control-label" })
        <div class="col-md-5">
            @Html.TextBoxFor(model => model.CacheDuration, new { @class = "form-control", type = "number", placeholder = Labels.CacheDuration })
            @Html.ValidationMessageFor(model => model.CacheDuration)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.FrontPageSize, new { @class = "col-md-2 control-label" })
        <div class="col-md-5">
            @Html.TextBoxFor(model => model.FrontPageSize, new { @class = "form-control", type="number", placeholder = Labels.PageSize })
            @Html.ValidationMessageFor(model => model.FrontPageSize)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.ThumbWidth, new { @class = "col-md-2 control-label" })
        <div class="col-md-5">
            @Html.TextBoxFor(model => model.ThumbWidth, new { @class = "form-control", type = "number", placeholder = Labels.ThumbWidth })
            @Html.ValidationMessageFor(model => model.ThumbWidth)
        </div>

    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.ThumbHeight, new { @class = "col-md-2 control-label" })
        <div class="col-md-5">
            @Html.TextBoxFor(model => model.ThumbHeight, new { @class = "form-control", type = "number", placeholder = Labels.ThumbHeight })
            @Html.ValidationMessageFor(model => model.ThumbHeight)
        </div>
    </div>
               

    <div class="form-group">
        <div class="col-sm-offset-2 col-md-3">
            <button type="submit" class="btn btn-primary" id="btnSave"><i class="fa fa-save fa-fw"></i>保存</button>
        </div>
        @*<div class="col-md-7">
                @TempData["message"]
            </div>*@
    </div>
}